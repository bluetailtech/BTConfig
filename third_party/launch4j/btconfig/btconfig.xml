<launch4jConfig>
  <headerType>gui</headerType>
  <jar>BTConfig.jar</jar>
  <dontWrapJar>false</dontWrapJar>
  <outfile>./BTConfig.exe</outfile>
  <errTitle>BTConfig</errTitle>
  <chdir>.</chdir>
  <icon>BTConfig.ico</icon>
  <restartOnCrash>true</restartOnCrash>
  <jre>
    <minVersion>1.8.0</minVersion>
  </jre>
  <splash>
    <file>BTConfig.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
</launch4jConfig>
